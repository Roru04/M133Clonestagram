@model IEnumerable<Clonestagram.Models.Post>

<h2>ShowPosts</h2>
@if (Model != null)
{
    foreach (Clonestagram.Models.Post post in Model)
    {
        <div class="jumbotron">
            <h3>@post.ApplicationUserName has posted</h3>
            <h2>@post.Title</h2>
            <p>@post.Content</p>

        </div>
    }

}
else
{
    <p>there arent any posts yet...</p>
}

